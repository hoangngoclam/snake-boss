  "scripts": {
    "dev": "nodemon",
    "start": "ts-node dist/server.js",
    "build": "tsc --p ./"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0"
  }
}
